{
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/api/works",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/work/*",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/ids",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/images/*",
      "dest": "index.js",
      "methods": ["GET"]
    }
  ]
}

